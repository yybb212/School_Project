CC = gcc
CFLAGS = -W -Wall
TARGET1 = 1.out
TARGET2 = 2.out
TARGET3 = 3.out
SRCS1 = 20141556_1.c
SRCS2 = 20141556_2.c
SRCS3 = 20141556_3.c
OBJECTS1 = 20141556_1.o
OBJECTS2 = 20141556_2.o
OBJECTS3 = 20141556_3.o

all: $(TARGET1) $(TARGET2) $(TARGET3)

$(TARGET1): $(OBJECTS1)
	$(CC) -o $(TARGET1) $(OBJECTS1) $(CFLAGS)

$(TARGET2): $(OBJECTS2)
	$(CC) -o $(TARGET2) $(OBJECTS2) $(CFLAGS)

$(TARGET3): $(OBJECTS3)
	$(CC) -o $(TARGET3) $(OBJECTS3) $(CFLAGS)

$(OBJECTS1): $(SRCS1)
	$(CC) -c $(SRCS1) $(CFLAGS)

$(OBJECTS2): $(SRCS2)
	$(CC) -c $(SRCS2) $(CFLAGS)

$(OBJECTS3): $(SRCS3)
	$(CC) -c $(SRCS3) $(CFLAGS)

clean:
	rm -rf *.o *.out
